{
    "name": "Thor plugin",
    "description": "Thor plugin",
    "version": "1.0",
    "permissions": ["contextMenus"],
    "background": {
        "scripts": ["background.js"],
	   "persistent": false
    },  
    "icons": { "16":"favicon.ico"},
    "content_scripts": [{
	    "matches": ["<all_urls>"],
	    "css": ["content.css"],
	    "js": ["content.js"]
	}],   
    "browser_action": {
	"default_title": "ThorFC",
	"default_popup": "browser_action/popup.html"
    },
    "manifest_version": 2
}
